{
  "name": "gatsby-starter-tech-blog",
  "version": "1.0.0",
  "description": "A Gatsby Starter Tech Blog template",
  "main": "index.js",
  "author": "Vimalraj Selvam",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "deploy": "gatsby build && s3-deploy './public/**' --cwd './public/' --bucket wtait.me  --deleteRemoved --gzip && npm run invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E5FSQIFA4P2Z9 --paths \"/*\""
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.9.4",
    "gatsby": "^2.18.21",
    "gatsby-cli": "^2.8.26",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-s3": "^0.2.5",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typography": "^2.2.2",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-prismjs": "^3.1.4",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.18",
    "prismjs": "^1.15.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-typography": "^0.16.18",
    "s3-deploy": "^1.3.0",
    "styled-components": "^4.1.2",
    "typography": "^0.16.18",
    "typography-theme-bootstrap": "^0.16.18"
  }
